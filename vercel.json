{
  "version": 2,
  "builds": [
    {
      "src": "src/main.server.ts",    
      "use": "@vercel/node"  ,        
      "src": "dist/browser/**/*",    
      "use": "@vercel/static"       
    }
  ],
  "routes": [
    {
      "src": "/api/.*",               
      "dest": "/src/main.server.ts"   
    },
    {
      "src": "/(.*)",               
      "dest": "/dist/browser/index.html"  
    }
  ]
}
